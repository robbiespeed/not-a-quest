<h1 id="title">
  {{partial 'art/title-mark'}}
</h1>
<h2>Characters: ({{model.length}})</h2>
<ul class="menu">
  {{#each model as |player|}}
    <li>{{#link-to 'edit' player.id}}{{player.name}}{{/link-to}}</li>
  {{/each}}
  <li>{{#link-to 'create'}}Create Character{{/link-to}}</li>
  <li>
    <button {{action 'createEntity'}}>
      Create Entity
    </button>
  </li>
</ul>
